<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    
    <name>WayTec</name>
    <description>
        O app do WayTec
    </description>
    <author email="lucascosta12367@gmail.com" href="https://waytec.online/static/img/logo-waytec-2.0.png">
        Time do WayTec
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
        <!--
            ldpi    : 36x36 px
            mdpi    : 48x48 px
            hdpi    : 72x72 px
            xhdpi   : 96x96 px
            xxhdpi  : 144x144 px
            xxxhdpi : 192x192 px
        -->
        <icon src="www/assets/img/logo-waytec-2.0.png" density="ldpi" />
        <icon src="www/assets/img/logo-waytec-2.0.png" density="mdpi" />
        <icon src="www/assets/img/logo-waytec-2.0.png" density="hdpi" />
        <icon src="www/assets/img/logo-waytec-2.0.png" density="xhdpi" />
        <icon src="www/assets/img/logo-waytec-2.0.png" density="xxhdpi" />
        <icon src="www/assets/img/logo-waytec-2.0.png" density="xxxhdpi" />
    </platform>

    <platform name="windows">
        <icon src="www/assets/img/logo-waytec-2.0.png" target="StoreLogo" />
        <icon src="www/assets/img/logo-waytec-2.0.png" target="Square30x30Logo" />
        <icon src="www/assets/img/logo-waytec-2.0.png" target="Square44x44Logo" />
        <icon src="www/assets/img/logo-waytec-2.0.png" target="Square70x70Logo" />
        <icon src="www/assets/img/logo-waytec-2.0.png" target="Square71x71Logo" />
        <icon src="www/assets/img/logo-waytec-2.0.png" target="Square150x150Logo" />
        <icon src="www/assets/img/logo-waytec-2.0.png" target="Square310x310Logo" />
        <icon src="www/assets/img/logo-waytec-2.0.png" target="Wide310x150Logo" />
    </platform>

    <platform name="ios">
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon src="www/assets/img/logo-waytec-2.0.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="www/assets/img/logo-waytec-2.0.png" width="60" height="60" />
        <icon src="www/assets/img/logo-waytec-2.0.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="www/assets/img/logo-waytec-2.0.png" width="76" height="76" />
        <icon src="www/assets/img/logo-waytec-2.0.png" width="152" height="152" />
        <!-- Spotlight Icon -->
        <icon src="www/assets/img/logo-waytec-2.0.png" width="40" height="40" />
        <icon src="www/assets/img/logo-waytec-2.0.png" width="80" height="80" />
        <!-- iOS 6.1 -->
        <!-- iPhone / iPod Touch -->
        <icon src="www/assets/img/logo-waytec-2.0.png" width="57" height="57" />
        <icon src="www/assets/img/logo-waytec-2.0.png" width="114" height="114" />
        <!-- iPad -->
        <icon src="www/assets/img/logo-waytec-2.0.png" width="72" height="72" />
        <icon src="www/assets/img/logo-waytec-2.0.png" width="144" height="144" />
        <!-- iPad Pro -->
        <icon src="www/assets/img/logo-waytec-2.0.png" width="167" height="167" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="www/assets/img/logo-waytec-2.0.png" width="29" height="29" />
        <icon src="www/assets/img/logo-waytec-2.0.png" width="58" height="58" />
        <icon src="www/assets/img/logo-waytec-2.0.png" width="87" height="87" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="www/assets/img/logo-waytec-2.0.png" width="50" height="50" />
        <icon src="www/assets/img/logo-waytec-2.0.png" width="100" height="100" />
        <!-- iPad Pro -->
        <icon src="www/assets/img/logo-waytec-2.0.png" width="167" height="167" />
    </platform>
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />

</widget>

